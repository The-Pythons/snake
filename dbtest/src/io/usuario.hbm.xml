<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
 <class name="io.Usuario" table="Usuario">
  <id column="User" name="user" type="string" />
  <property column="Password" name="password" not-null="true" type="byte[]" />
  <property column="Estado" name="estado" not-null="true" type="int"></property>
 </class>
</hibernate-mapping>